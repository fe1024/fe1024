.fe-photo-frame
  padding 32px 24px;
  font-size 16px;
  background-color #fafafa;
  display inline-block;
  width 240px;
  height 360px;
  boxshadow();
  background-position center 0;
  background-repeat no-repeat;
  background-size cover;
  &-title
    font-weight 300;
  &-body
    text-align left;
    padding-top 24px;
  &.small
    width 180px;
    height 270px;